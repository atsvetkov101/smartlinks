  "scripts": {
    "clean:dist": "rimraf ./dist",
    "prebuild": "npm run clean:dist",
    "build": "nest build",
    "db-migration-create": "npx sequelize-cli migration:generate --name",
    "db-migrate": "npx sequelize-cli db:migrate",
    "db-migrate-down": "npx sequelize-cli db:migrate:undo",
    "db-seed-create": "npx sequelize-cli seed:generate --name",
    "db-seed": "npx sequelize-cli db:seed:all",
    "db-seed-down": "npx sequelize-cli db:seed:undo",
    "prestart:api:dev": "npm run build",
    "start:api:dev": "nest start --entryFile src/entrypoints/api/main --watch",
    "start:api:production": "node dist/src/entrypoints/api/main",
    "prestart:resolver:dev": "npm run build",
    "start:resolver:dev": "nest start --entryFile src/entrypoints/resolver/main --watch",
    "start:resolver:production": "node dist/src/entrypoints/resolver/main",
    "start-test1": "node dist/src/core/chain-of-responsibility/chain-of-responsibility-concept",
    "pretest": "npm run build",
    "test-mocha": "mocha dist/test/**/*.spec.js --timeout 60000 --exit",
    "prenyc": "npm run build",
    "nyc": "nyc mocha dist/test/**/*.spec.js --timeout 60000 --exit",
    "ts-test": "ts-mocha test/**/*.spec.ts --timeout 60000",
    "preistanbul": "npm run build",
    "istanbul": "nyc mocha dist/test/**/*.js --timeout 60000 --exit",
    "istanbul1": "nyc mocha dist/test/**/*.js --timeout 60000 --exit",
    "test-jest": "npx jest",
    "test-jest-coverage": "npx jest --coverage",
    "test": "jest",
    "test-jest-npm": "npm test -- --coverage",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "docker-build": "sudo docker build -t smartlink ."
  },
