    "@nestjs/schedule": "^5.0.1",
    "@nestjs/sequelize": "^11.0.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/testing": "^11.1.0",
    "@types/chai": "^5.2.2",
    "@types/sinon": "^17.0.4",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.5",
    "chai": "^5.2.0",
    "class-transformer": "^0.5.1",
