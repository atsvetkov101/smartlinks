{
  "name": "smartlink",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "db-migration-create": "npx sequelize-cli migration:generate --name",
    "db-migrate": "npx sequelize-cli db:migrate",
    "db-migrate-down": "npx sequelize-cli db:migrate:undo",
    "db-seed-create": "npx sequelize-cli seed:generate --name",
    "db-seed": "npx sequelize-cli db:seed:all",
    "db-seed-down": "npx sequelize-cli db:seed:undo",
    "prestart:api:dev": "nest start --entryFile src/entrypoints/api/main --watch",
    "start:api:dev": "yarn start build",
    "start:api:production": "node dist/src/entrypoints/api/main",
    "pretest": "npm run build",
    "test": "mocha dist/test/**/*.js --timeout 60000 --exit",
    "test-jest": "npx jest",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "docker-build": "sudo docker build -t smartlink ."
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
